<UiButton Variant="UiButtonVariant.Default" Class="font-bold shadow-md shadow-primary-200 group" OnClick="OnClick">
    <i class="fa-solid fa-wand-magic-sparkles mr-2 text-primary-300 group-hover:rotate-12 transition-transform"></i>
    <span>@Name</span>
</UiButton>


@code{
    [Parameter]
    public EventCallback<MouseEventArgs> OnClick { get; set; }

    [Parameter]
    public string? Name { get; set; } = "Generate Data";
}